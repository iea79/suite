// begin stepFirst
.stepsContent.stepFirst
    .stepFirst__left
        .box__title
            .box__name
                i.icon.icon_out 
                span Отдаете

        .stepFirst__list
            -var inner = [{name: 'Examplename', icon: 'img/bitcon.png', value: 'Btc', cours: '1'}, {name: 'Examplename', icon: 'img/bitconh2.png', value: 'Btc', cours: '1'}, {name: 'Master Cart', icon: 'img/master-cart.png', value: 'Руб', cours: '1'}, {name: 'Qiwi', icon: 'img/qiwi.png', value: 'Руб', cours: '1'}, {name: 'Examplename', icon: 'img/tele2.png', value: 'Руб', cours: '1'}, {name: 'Visa', icon: 'img/visa.png', value: 'Руб', cours: '1'}, {name: 'Examplename', icon: 'img/wallet.png', value: 'Btc', cours: '1'}, {name: 'Яндекс Деньги', icon: 'img/yandex-wallet.png', value: 'Руб', cours: '1'}]
            each item, i in inner
                .stepFirst__item(data-item-in data-id="in-"+i+"" data-cours= item.cours data-icon= item.icon data-name= item.name data-value= item.value)
                    .stepFirst__info
                        .stepFirst__icon
                            img(src= item.icon alt="")
                        .stepFirst__name= item.name

    .stepFirst__right
        .box__title
            .box__name
                i.icon.icon_in 
                span Получаете
            .box__props
                .box__cours Курс
                .box__reserve Резерв

        .stepFirst__list
