            each item, i in outer
                .stepFirst__item(data-item-out data-id="out-"+i+"" data-cours= item.cours data-icon= item.icon data-name= item.name data-value= item.value data-reserv= item.reserv data-timer= item.time)
                    .stepFirst__info
                        .stepFirst__icon
                            img(src= item.icon alt="")
                        .stepFirst__name= item.name
                    .stepFirst__props
                        .stepFirst__cours= item.cours
                        .stepFirst__reserve= item.reserv

    // begin stepsUpdate
    .stepsUpdate
        .stepsUpdate__icon
            i.icon.icon_spinn

    // end stepsUpdate

